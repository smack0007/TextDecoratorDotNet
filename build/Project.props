<Project>
  <PropertyGroup>
    <RepositoryUrl>https://github.com/smack0007/TextDecoratorDotNet.git</RepositoryUrl>

    <PackageProjectUrl>https://github.com/smack0007/TextDecoratorDotNet/</PackageProjectUrl>
    <PackageTags></PackageTags>

    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <!-- Warnings related to Nullable should be treated as errors. -->
    <WarningsAsErrors>$(WarningsAsErrors);CS8618</WarningsAsErrors>
  </PropertyGroup>
</Project>
